<!doctype html>
<!--[if lte IE 9]>
<html class="lte-ie9" lang="fr"> <![endif]-->
<!--[if gt IE 9]><!-->
<html lang="fr"> <!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="initial-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Remove Tap Highlight on Windows Phone IE -->
    <meta name="msapplication-tap-highlight" content="no"/>
    <title>{% block title %}Grifoflex Tunisie{% endblock %}</title>
    {% block css %}
        {% stylesheets output="css/compiled/mainBase.css" filter="cssrewrite"
        'bundles/grifoflextunisie/bower_components/uikit/css/uikit.almost-flat.min.css'
        'bundles/grifoflextunisie/assets/css/login_page.min.css'
        'bundles/grifoflextunisie/assets/css/ie.css'
        'bundles/grifoflextunisie/assets/css/themes/themes_combined.min.css' %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
        {% endstylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('icon.ico') }}"/>
    {% endblock %}
</head>
<body class="login_page">
<div>
    {% block body %} {% endblock %}
</div>
{% block js %}
    {% javascripts output="js/compiled/mainBase.js"
    'bundles/grifoflextunisie/assets/js/common.min.js'
    'bundles/grifoflextunisie/assets/js/uikit_custom.min.js'
    'bundles/grifoflextunisie/assets/js/altair_admin_common.min.js'
    'bundles/grifoflextunisie/assets/js/pages/login.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>